<navascript>
	<map.sqlquery datasource="'dvdrental'">
		<sqlquery.query xml:space="preserve">
                SELECT * FROM   actor
            	</sqlquery.query>
		<!-- <sqlquery.addParameter value="100" /> -->
		<message name="Actors" type="array">
			<sqlquery.resultSet>
		<property name="FirstName" direction="out">
			<expression value="$columnValue('first_name')" />
		</property>
		<property name="LastName" direction="out">
			<expression value="$columnValue('last_name')" />
		</property>

			</sqlquery.resultSet>
			</message>
	</map.sqlquery>
</navascript>