<?xml version="1.0" encoding="UTF-8"?>

<!-- XTSL test caller script for the KafkaMap. -->

<tsl>

    <message name="kafkamap">
        <map object="com.dexels.navajo.adapter.KafkaMap">            
            <field name="key">
                <expression value="'my-key-tsl'"/>
            </field>
            
            <field name="doPost">
                <expression value="'my-topic-tsl'"/>
            </field>

            <property name="response" type="string" direction="out">
                <expression value="$response" />
            </property>
        </map>
    </message>

</tsl>
